{
  "name": "Frappe Bench",
  "forwardPorts": [
    8000,
    9000,
    6787
  ],
  "remoteUser": "frappe",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.live-server",
        "grapecity.gc-excelviewer",
        "mtxr.sqltools",
        "visualstudioexptteam.vscodeintellicode"
      ],
      "settings": {
        "terminal.integrated.profiles.linux": {
          "frappe bash": {
            "path": "/bin/bash"
          }
        },
        "terminal.integrated.defaultProfile.linux": "bash",
        "debug.node.autoAttach": "disabled"
      }
    }
  },
  "dockerComposeFile": "./docker-compose.yml",
  "service": "frappe",
  "workspaceFolder": "/home/frappe/frappe-bench/apps/tattoo_shop",
  "mounts": [
    "source=${localEnv:HOME}${localEnv:USERPROFILE}/.ssh,target=/home/frappe/.ssh,type=bind,consistency=cached"
  ],
  "postCreateCommand": "cd /home/frappe/frappe-bench; bench new-site ${SITE_NAME} --db-host ${DB_HOST} --db-root-password ${DB_PASSWORD} --admin-password ${ADMIN_PASSWORD}"
}